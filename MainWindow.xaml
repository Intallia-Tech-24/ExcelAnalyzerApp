<Window x:Class="ExcelAnalyzerApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Excel Analyzer" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button x:Name="BrowseButton" Content="Browse Excel File" Click="BrowseButton_Click" Width="150" Margin="10"/>
            <ComboBox x:Name="WorksheetComboBox" Width="150" Margin="10" SelectionChanged="WorksheetComboBox_SelectionChanged"/>
            <ListBox x:Name="ResultsListBox" Width="700" Height="300" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>